{
  "multipart": [
	{   "when": { "floor": "true" },
		"apply": { "model": "conveyance:block/catwalk" }
	},
	{   "when": { "north": "false" },
		"apply": { "model": "conveyance:block/catwalk_wall" }
	},
	{   "when": { "east": "false" },
		"apply": { "model": "conveyance:block/catwalk_wall", "y": 90 }
	},
	{   "when": { "south": "false" },
		"apply": { "model": "conveyance:block/catwalk_wall", "y": 180 }
	},
	{   "when": { "west": "false" },
		"apply": { "model": "conveyance:block/catwalk_wall", "y": 270 }
	},
	{   "when": { "north": "true" },
		"apply": { "model": "conveyance:block/catwalk_inside" }
	},
	{   "when": { "east": "true" },
		"apply": { "model": "conveyance:block/catwalk_inside", "y": 90, "uvlock": true }
	},
	{   "when": { "south": "true" },
		"apply": { "model": "conveyance:block/catwalk_inside", "y": 180, "uvlock": true }
	},
	{   "when": { "west": "true" },
		"apply": { "model": "conveyance:block/catwalk_inside", "y": 270, "uvlock": true }
	}
  ]
}
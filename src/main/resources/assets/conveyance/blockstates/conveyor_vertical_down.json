{
  "multipart": [
	{   "when": { "facing": "north" },
		"apply": { "model": "conveyance:block/conveyor_vertical_down" }
	},
	{   "when": { "facing": "east" },
		"apply": { "model": "conveyance:block/conveyor_vertical_down", "y": 90 }
	},
	{   "when": { "facing": "south" },
		"apply": { "model": "conveyance:block/conveyor_vertical_down", "y": 180 }
	},
	{   "when": { "facing": "west" },
		"apply": { "model": "conveyance:block/conveyor_vertical_down", "y": 270 }
	},
	{   "when": { "facing": "north", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension_down" }
	},
	{   "when": { "facing": "east", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension_down", "y": 90 }
	},
	{   "when": { "facing": "south", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension_down", "y": 180 }
	},
	{   "when": { "facing": "west", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension_down", "y": 270 }
	},
	{   "when": { "facing": "north", "conveyor": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_connector_down" }
	},
	{   "when": { "facing": "east", "conveyor": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_connector_down", "y": 90 }
	},
	{   "when": { "facing": "south", "conveyor": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_connector_down", "y": 180 }
	},
	{   "when": { "facing": "west", "conveyor": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_connector_down", "y": 270 }
	}
  ]
}
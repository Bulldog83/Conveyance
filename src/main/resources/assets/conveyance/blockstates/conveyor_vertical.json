{
  "multipart": [
	{   "when": { "facing": "north" },
		"apply": { "model": "conveyance:block/conveyor_vertical" }
	},
	{   "when": { "facing": "east" },
		"apply": { "model": "conveyance:block/conveyor_vertical", "y": 90 }
	},
	{   "when": { "facing": "south" },
		"apply": { "model": "conveyance:block/conveyor_vertical", "y": 180 }
	},
	{   "when": { "facing": "west" },
		"apply": { "model": "conveyance:block/conveyor_vertical", "y": 270 }
	},
	{   "when": { "facing": "north", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension" }
	},
	{   "when": { "facing": "east", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension", "y": 90 }
	},
	{   "when": { "facing": "south", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension", "y": 180 }
	},
	{   "when": { "facing": "west", "front": "true" },
		"apply": { "model": "conveyance:block/conveyor_vertical_extension", "y": 270 }
	}
  ]
}